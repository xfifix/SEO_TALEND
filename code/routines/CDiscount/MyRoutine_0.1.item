		String URL="";
		String debutURL="";
		String partie1="";
		String partie2="";
		String partie3="";
		Integer nbcar=0;
		Integer nbcar_3=0;
		Integer nbcar_2=0;
		Integer nbcar_1=0;
		
		
		

		if (pays==1)
		{
		debutURL="http://ak.cdiscount.com/pdt/";
